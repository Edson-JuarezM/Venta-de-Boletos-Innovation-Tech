@model IEnumerable<SuperChampiniones.Models.Partido>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.EquipoA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EquipoB)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_Hora)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UrlEscudA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UrlEscudB)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EquipoA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EquipoB)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_Hora)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UrlEscudA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UrlEscudB)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
